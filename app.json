{
    "expo": {
        "name": "companion-app",
        "slug": "companion-app",
        "version": "1.0.0",
        "orientation": "portrait",
        "icon": "./assets/icon.png",
        "userInterfaceStyle": "light",
        "newArchEnabled": true,
        "splash": {
            "image": "./assets/splash-icon.png",
            "resizeMode": "contain",
            "backgroundColor": "#6495ed"
        },
        "plugins": [
            "@react-native-google-signin/google-signin",
            "@react-native-firebase/app",
            "@react-native-firebase/auth",
            "expo-notifications"
        ],
        "ios": {
            "supportsTablet": true,
            "bundleIdentifier": "com.amaurychong.companionapp",
            "googleServicesFile": "./GoogleService-Info.plist"
        },
        "android": {
            "adaptiveIcon": {
                "foregroundImage": "./assets/adaptive-icon.png",
                "backgroundColor": "#6495ed"
            },
            "package": "com.amaurychong.companionapp",
            "googleServicesFile": "./google-services.json",
            "versionCode": 1,
            "config": {
                "googleMaps": {
                    "apiKey": "AIzaSyDTE9j9PnRUVueaUoDf7G4cabUdXwhu6MY"
                }
            }
        },
        "web": {
            "favicon": "./assets/favicon.png"
        },
        "extra": {
            "eas": {
                "projectId": "8ad98b72-0ad2-4c1e-b6fc-da11368e442d"
            }
        },
        "scheme": "companionapp",
        "platforms": [
            "ios",
            "android",
            "web"
        ]
    }
}
